{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "member-ordering": [
      true,
      {
          "order": [
              "static-field",
              "instance-field",
              "constructor",
              "instance-method"
          ]
      }
    ]
  }
}
